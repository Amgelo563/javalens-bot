{
  "name": "javalens-bot",
  "description": "A lightweight Discord bot for searching on Javadocs through slash commands.",
  "version": "0.0.1",
  "main": "dist/index.js",
  "type": "module",
  "private": true,
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "pnpm run clean && tsc",
    "start": "node --require source-map-support/register dist/index.js",
    "lint": "eslint src --fix",
    "clean-start": "pnpm run build && pnpm run start"
  },
  "license": "MIT",
  "dependencies": {
    "@discordjs/collection": "2.1.1",
    "@discordjs/core": "2.2.0",
    "@discordjs/formatters": "0.6.1",
    "@discordjs/rest": "2.5.1",
    "@discordjs/ws": "2.0.3",
    "@pushcorn/hocon-parser": "1.3.1",
    "fuzzbunny": "1.0.1",
    "javadocs-scraper": "2.1.2",
    "neverthrow": "8.2.0",
    "piscina": "5.1.3",
    "queue-promise": "2.2.1",
    "source-map-support": "0.5.21",
    "tslog": "4.9.3",
    "turndown": "7.2.0",
    "valibot": "0.37.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-conventional": "19.5.0",
    "@commitlint/types": "19.5.0",
    "@eslint/compat": "1.1.1",
    "@eslint/eslintrc": "3.1.0",
    "@eslint/js": "9.7.0",
    "@ninoseki/eslint-plugin-neverthrow": "0.0.1",
    "@types/eslint__js": "8.42.3",
    "@types/node": "20.14.10",
    "@types/validator": "13.12.0",
    "@types/turndown": "5.0.5",
    "@typescript-eslint/eslint-plugin": "8.18.2",
    "@typescript-eslint/parser": "8.18.2",
    "eslint": "9.7.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.7.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-require-extensions": "0.1.3",
    "globals": "15.8.0",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "prettier": "3.5.3",
    "rimraf": "6.0.1",
    "typescript": "5.6.3",
    "typescript-eslint": "8.18.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@9.5.0"
}
